-- ts file was generated at https://discord.gg/URavuvVNTR

local Whitelist = {
    'Micaxan',

',
}

local function CheckWhitelist(PlayerName)
    local Index, Value, Iterator = ipairs(Whitelist)

    while true do
        local CurrentPlayer

        Iterator, CurrentPlayer = Index(Value, Iterator)

        if Iterator == nil then
            break
        end
        if PlayerName == CurrentPlayer then
            return true
        end
    end

    return false
end

local LocalPlayer = game.Players.LocalPlayer

if not CheckWhitelist(LocalPlayer.Name) then
    LocalPlayer:Kick("Don't execute this script nigga, I'll rebirth your ass + dox u if I ain't whitelisted")
end

local UiLibrary = loadstring(game:HttpGet('https://raw.githubusercontent.com/memejames/elerium-v2-ui-library//main/Library', true))():AddWindow('KTM HUB - BOOSTER VERSION', {
    main_color = Color3.fromRGB(255, 105, 180),
    min_size = Vector2.new(510, 346),
    can_resize = false,
})
local HomeTab = UiLibrary:AddTab('Home')

HomeTab:Show()
HomeTab:AddLabel('Welcome to the Booster Version of KTM HUB!')
HomeTab:AddLabel('Thanks for Boosting the server!')
HomeTab:AddLabel('Since you Boosted the server')

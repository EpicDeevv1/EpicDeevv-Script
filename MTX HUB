local KillingTab = UiLibrary:AddTab('Killing')

KillingTab:Show()
KillingTab:AddLabel('I know this tab is pretty empty')
KillingTab:AddLabel('So if you want anything added to this tab')
KillingTab:AddLabel('Go to the discord server to the suggestions channel!')

local Whitelist = {}
local BooleanValue = false

local function Function(Parameter)
    local LocalPlayer = game.Players.LocalPlayer
    local Backpack = LocalPlayer:FindFirstChild('Backpack')
    local Tool = Backpack and Backpack:FindFirstChild(Parameter)

    if Tool then
        LocalPlayer.Character.Humanoid:EquipTool(Tool)
    end
end
local function Function()
    local LocalPlayer = game.Players.LocalPlayer
    local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local HumanoidRootPart = Character:WaitForChild('HumanoidRootPart')

    Function('Punch')

    while BooleanValue do
        local Player, Player, Player = ipairs(game.Players:GetPlayers())

        while true do
            local Player

            Player, Player = Player(Player, Player)

            if Player == nil then
                break
            end
            if Player ~= LocalPlayer and not table.find(Whitelist, Player.Name) then
                pcall(function()
                    local Character = Player.Character

                    if Character then
                        Character = Player.Character:FindFirstChild('HumanoidRootPart')
                    end
                    if Character then
                        Character.Size = Vector3.new(20, 20, 20)
                        Character.Transparency = 1
                        Character.Color = Color3.new(1, 0, 0)
                        Character.Material = Enum.Material.Neon
                        Character.CanCollide = false
                        Character.CFrame = CFrame.new(1955.3785, 1.7816, 6170.521)

                        local PunchTool = Character:FindFirstChild('Punch')

                        if PunchTool then
                            PunchTool:Activate()
                        end
                    end
                end)
            end
        end

        HumanoidRootPart.CFrame = CFrame.new(1953.2662, 1.7816, 6186.1226)

        wait(0.01)
    end
end
local function Function()
    local LocalPlayer = game.Players.LocalPlayer

    LocalPlayer.CharacterAdded:Connect(function()
        wait(0.5)

        if BooleanValue then
            local Character = LocalPlayer.Character

            if Character then
                Character = Character:WaitForChild('HumanoidRootPart')
            end

            Character.CFrame = CFrame.new(1953.2662, 1.7816, 6186.1226)

            Function('Punch')
            spawn(Function)
        end
    end)
end

KillingTab:AddSwitch('Auto Kill Everyone', function(Parameter)
    BooleanValue = Parameter

    if BooleanValue then
        spawn(Function)
        print('Auto Kill On')
    else
        print('Auto Kill Off')
    end
end):Set(false)

local WhitelistPlayersDropdown = KillingTab:AddDropdown('Whitelist Players', function(PlayerName)
    if table.find(Whitelist, PlayerName) then
        local Player, Player, Player = ipairs(Whitelist)

        while true do
            local UnknownValue

            Player, UnknownValue = Player(Player, Player)

            if Player == nil then
                break
            end
            if UnknownValue == PlayerName then
                table.remove(Whitelist, Player)

                break
            end
        end
    else
        table.insert(Whitelist, PlayerName)
    end

    print('Updated Whitelist:', Whitelist)
end)
local Player, Player, Player = ipairs(game.Players:GetPlayers())

while true do
    local UnknownValue

    Player, UnknownValue = Player(Player, Player)

    if Player == nil then
        break
    end
    if UnknownValue.Name ~= game.Players.LocalPlayer.Name then
        WhitelistPlayersDropdown:Add(UnknownValue.Name)
    end
end

game.Players.PlayerAdded:Connect(function(Parameter1)
    if Parameter1.Name ~= game.Players.LocalPlayer.Name then
        WhitelistPlayersDropdown:Add(Parameter1.Name)
    end
end)
game.Players.PlayerRemoving:Connect(function(Parameter2)
    if Parameter2.Name ~= game.Players.LocalPlayer.Name then
        WhitelistPlayersDropdown:Remove(Parameter2.Name)
    end
end)
Function()

local RocksTab = UiLibrary:AddTab('Rocks')

RocksTab:Show()
RocksTab:AddSwitch('Ancient Jungle Rock', function(Parameter)
    if Parameter then
        local LocalPlayer = game.Players.LocalPlayer
        local LeftHand = (LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()):WaitForChild('LeftHand')
        local Rock = game.Workspace.machinesFolder:FindFirstChild('Ancient Jungle Rock'):FindFirstChild('Rock')

        _G.jungleautorock = true

        while _G.jungleautorock do
            if Rock then
                Rock.Size = Vector3.new(2, 1, 1)
                Rock.Transparency = 1

                local GuiElement, GuiElement, GuiElement = pairs(Rock.rockGui:GetChildren())

                while true do
                    local UnknownValue

                    GuiElement, UnknownValue = GuiElement(GuiElement, GuiElement)

                    if GuiElement == nil then
                        break
                    end

                    UnknownValue.Visible = false
                end

                Rock.CanCollide = false

                if Rock:FindFirstChild('rockEmitter') then
                    Rock.rockEmitter:Destroy()
                end
                if Rock:FindFirstChild('hoopParticle') then
                    Rock.hoopParticle:Destroy()
                end
                if Rock:FindFirstChild('lavaParticle') then
                    Rock.lavaParticle:Destroy()
                end

                Rock.CFrame = LeftHand.CFrame
            else
                warn('Rock not found.')
            end

            wait()
        end
    else
        local Rock, Error = pcall(function()
            local Rock = game.Workspace.machinesFolder:FindFirstChild('Ancient Jungle Rock'):FindFirstChild('Rock')

            _G.jungleautorock = false

            if Rock then
                local GuiElement, GuiElement, GuiElement = pairs(Rock.rockGui:GetChildren())

                while true do
                    local UnknownValue

                    GuiElement, UnknownValue = GuiElement(GuiElement, GuiElement)

                    if GuiElement == nil then
                        break
                    end

                    UnknownValue.Visible = true
                end

                Rock.CanCollide = true
                Rock.Transparency = 0
                Rock.CFrame = CFrame.new(Rock.originalPosition.Value)
                Rock.Size = Vector3.new(23.18409538269043, 20.20000648498535, 24.44658088684082)
            else
                warn('Rock not found in Ancient Jungle.')
            end
        end)

        if not Rock then
            warn('Error turning Ancient Jungle Rock off.')
        end
    end
end):Set(false)

RocksTab:AddSwitch('Frozen Rock', function(Parameter3)
    local LocalPlayer3 = game.Players.LocalPlayer
    local LeftHand3 = (LocalPlayer3.Character or LocalPlayer3.CharacterAdded:Wait()):WaitForChild('LeftHand')
    local FrozenRock = game.Workspace.machinesFolder:FindFirstChild('Frozen Rock'):FindFirstChild('Rock')

    if Parameter3 then
        _G.frozenautorock = true

        while _G.frozenautorock do
            if FrozenRock then
                FrozenRock.Size = Vector3.new(2, 1, 1)
                FrozenRock.Transparency = 1
                FrozenRock.CanCollide = false

                local ChildItem23, ChildItem24, ChildItem25 = pairs(FrozenRock.rockGui:GetChildren())

                while true do
                    local UnknownValue8

                    ChildItem25, UnknownValue8 = ChildItem23(ChildItem24, ChildItem25)

                    if ChildItem25 == nil then
                        break
                    end

                    UnknownValue8.Visible = false
                end

                local ParticleName, ParticleName1, ParticleName2 = ipairs({
                    'rockEmitter',
                    'hoopParticle',
                    'lavaParticle',
                })

                while true do
                    local UnknownValue9

                    ParticleName2, UnknownValue9 = ParticleName(ParticleName1, ParticleName2)

                    if ParticleName2 == nil then
                        break
                    end
                    if FrozenRock:FindFirstChild(UnknownValue9) then
                        FrozenRock[UnknownValue9]:Destroy()
                    end
                end

                FrozenRock.CFrame = LeftHand3.CFrame
            else
                warn('Frozen Rock not found.')
            end

            wait()
        end
    else
        _G.frozenautorock = false

        pcall(function()
            if FrozenRock then
                local ChildItem26, ChildItem27, ChildItem28 = pairs(FrozenRock.rockGui:GetChildren())

                while true do
                    local UnknownValue10

                    ChildItem28, UnknownValue10 = ChildItem26(ChildItem27, ChildItem28)

                    if ChildItem28 == nil then
                        break
                    end

                    UnknownValue10.Visible = true
                end

                FrozenRock.CanCollide = true
                FrozenRock.Transparency = 0
                FrozenRock.Size = Vector3.new(80.6851577758789, 70.2999496459961, 85.07884979248047)
                FrozenRock.CFrame = CFrame.new(FrozenRock.originalPosition.Value)

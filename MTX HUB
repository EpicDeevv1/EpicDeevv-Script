            local TinyRock1 = game.Workspace.machinesFolder:FindFirstChild('Tiny Rock'):FindFirstChild('Rock')

            if TinyRock1 then
                local ChildItem32, ChildItem33, ChildItem34 = pairs(TinyRock1.rockGui:GetChildren())

                while true do
                    local UnknownValue13

                    ChildItem34, UnknownValue13 = ChildItem32(ChildItem33, ChildItem34)

                    if ChildItem34 == nil then
                        break
                    end

                    UnknownValue13.Visible = true
                end

                TinyRock1.CanCollide = true
                TinyRock1.Transparency = 0
                TinyRock1.Size = Vector3.new(23.18409538269043, 20.20000648498535, 24.44658088684082)
                TinyRock1.CFrame = CFrame.new(TinyRock1.originalPosition.Value)
            else
                warn('Tiny Rock not found in Muscle King Mountain.')
            end
        end)
    end
end):Set(false)


HomeTab:AddLabel('Anyways bye and enjoy the script! - MLR TAKEN')

local FarmingTab = UiLibrary:AddTab('Farming')

FarmingTab:Show()

local FastPunchToggle = FarmingTab:AddSwitch('Fast Punch', function(Enabled)
    local LocalPlayer = game.Players.LocalPlayer
    local PunchTool = LocalPlayer.Backpack:FindFirstChild('Punch')
    local PlayerCharacter = game.Workspace:FindFirstChild(LocalPlayer.Name)
    local UnknownValue

    if PlayerCharacter then
        UnknownValue = PlayerCharacter:FindFirstChild('Punch')
    else
        UnknownValue = nil
    end
    if Enabled then
        if PunchTool and PunchTool:FindFirstChild('attackTime') then
            PunchTool.attackTime.Value = 0
        elseif UnknownValue and UnknownValue:FindFirstChild('attackTime') then
            UnknownValue.attackTime.Value = 0
        end
    elseif PunchTool and PunchTool:FindFirstChild('attackTime') then
        PunchTool.attackTime.Value = 0.35
    elseif UnknownValue and UnknownValue:FindFirstChild('attackTime') then
        UnknownValue.attackTime.Value = 0.35
    end
end)
local FastPunchState = FastPunchToggle

FastPunchToggle.Set(FastPunchState, false)

local AutoPunchToggle = FarmingTab:AddSwitch('Auto Punch', function(Enabled)
    if Enabled then
        local LocalPlayer = game.Players.LocalPlayer
        local PlayerName = LocalPlayer.Name
        local PunchTool = LocalPlayer.Backpack:FindFirstChild('Punch') or game.Workspace:FindFirstChild(PlayerName):FindFirstChild('Punch')

        _G.punchanim = true

        while _G.punchanim do
            if PunchTool then
                if PunchTool.Parent ~= game.Workspace:FindFirstChild(PlayerName) then
                    PunchTool.Parent = game.Workspace:FindFirstChild(PlayerName)
                end

                PunchTool:Activate()
                wait()
            else
                warn('Punch tool not found')

                _G.punchanim = false
            end
        end
    else
        _G.punchanim = false
    end
end)
local AutoPunchState = AutoPunchToggle

AutoPunchToggle.Set(AutoPunchState, false)
FarmingTab:AddSwitch('Auto Punch / Fast Punch', function(Parameter1)
    FastPunchToggle:Set(Parameter1)
    AutoPunchToggle:Set(Parameter1)
end):Set(false)
FarmingTab:AddSwitch('Auto Strength', function(Parameter2)
    isAutoStrength = Parameter2

    local LocalPlayer = game.Players.LocalPlayer
    local Backpack = LocalPlayer:FindFirstChild('Backpack')

    while isAutoStrength do
        wait(0.1)

        local WeightTool = Backpack and Backpack:FindFirstChild('Weight')

        if WeightTool then
            LocalPlayer.Character.Humanoid:EquipTool(WeightTool)
        end

        game:GetService('Players').LocalPlayer.muscleEvent:FireServer(unpack({
            'rep',
        }))
    end

    if not isAutoStrength then
        local Tool = LocalPlayer.Character:FindFirstChildOfClass('Tool')

        if Tool and Tool.Name == 'Weight' then
            Tool.Parent = Backpack
        end
    end
end):Set(false)

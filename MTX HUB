local OpStuffTab = UiLibrary:AddTab('Op Stuff')

OpStuffTab:Show()
OpStuffTab:AddLabel('Nothing in this tab is fully developed')
OpStuffTab:AddLabel('So some of the functions may be buggy')
OpStuffTab:AddLabel('If someting doesnt work dm me (MLR TAKEN)')
OpStuffTab:AddSwitch('Fast glitch (equip punch for it to work)', function(Parameter)
    if Parameter then
        local LocalPlayer = game.Players.LocalPlayer
        local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local LeftHand = Character:WaitForChild('LeftHand')

        Character:WaitForChild('RightHand')

        local Rock = game.Workspace.machinesFolder:FindFirstChild('Muscle King Mountain'):FindFirstChild('Rock')

        _G.musclekingautorock = true

        while _G.musclekingautorock do
            game:GetService('Players').LocalPlayer.muscleEvent:FireServer('punch', 'leftHand')
            game:GetService('Players').LocalPlayer.muscleEvent:FireServer('punch', 'rightHand')

            if Rock then
                game:GetService('Players').LocalPlayer.muscleEvent:FireServer('punch', 'Rock')

                Rock.Size = Vector3.new(2, 1, 1)
                Rock.Transparency = 1
                Rock.CanCollide = false

                local GuiChild, GuiChild1, GuiChild2 = pairs(Rock.rockGui:GetChildren())

                while true do
                    local UnknownValue

                    GuiChild2, UnknownValue = GuiChild(GuiChild1, GuiChild2)

                    if GuiChild2 == nil then
                        break
                    end

                    UnknownValue.Visible = false
                end

                local ParticleName, ParticleName1, ParticleName2 = ipairs({
                    'rockEmitter',
                    'hoopParticle',
                    'lavaParticle',
                })

                while true do
                    local UnknownValue1

                    ParticleName2, UnknownValue1 = ParticleName(ParticleName1, ParticleName2)

                    if ParticleName2 == nil then
                        break
                    end
                    if Rock:FindFirstChild(UnknownValue1) then
                        Rock[UnknownValue1]:Destroy()
                    end
                end

                Rock.CFrame = LeftHand.CFrame
            else
                warn('Rock not found.')
            end

            local SomeOtherObject = game.Workspace:FindFirstChild('SomeOtherObject')

            if SomeOtherObject then
                game:GetService('Players').LocalPlayer.muscleEvent:FireServer('touch', SomeOtherObject)
            end

            wait(0.03)
        end
    else
        _G.musclekingautorock = false

        pcall(function()
            local Rock1 = game.Workspace.machinesFolder:FindFirstChild('Muscle King Mountain'):FindFirstChild('Rock')

            if Rock1 then
                local GuiChild3, GuiChild4, GuiChild5 = pairs(Rock1.rockGui:GetChildren())

                while true do
                    local UnknownValue2

                    GuiChild5, UnknownValue2 = GuiChild3(GuiChild4, GuiChild5)

                    if GuiChild5 == nil then
                        break
                    end

                    UnknownValue2.Visible = true
                end

                Rock1.CanCollide = true
                Rock1.Transparency = 0
                Rock1.CFrame = CFrame.new(Rock1.originalPosition.Value)
                Rock1.Size = Vector3.new(141.97381591796875, 123.69998931884766, 149.70497131347656)
            else
                warn('Rock not found in Muscle King Mountain.')
            end
        end)
    end
end):Set(false)
OpStuffTab:AddSwitch('Infinite Godmode', function(Parameter1)
    spamEnabled = Parameter1

    if spamEnabled then
        StartSpamming()
    end
end):Set(false)

function StartSpamming()
    while spamEnabled do
        game:GetService('ReplicatedStorage'):WaitForChild('rEvents'):WaitForChild('brawlEvent'):FireServer(unpack({
            'joinBrawl',
        }))
        wait(spamRate)
    end
end

OpStuffTab:AddButton('Load MTX Hub Stat Changer', function()
    local ScreenGui = Instance.new('ScreenGui')
    local Frame = Instance.new('Frame')
    local TextButton = Instance.new('TextButton')
    local UICorner = Instance.new('UICorner')
    local UIGradient = Instance.new('UIGradient')

    ScreenGui.Parent = game:GetService('CoreGui')
    ScreenGui.Name = 'DeltaSquareUI'
    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.new(0, 0, 0)
    Frame.BackgroundTransparency = 0.5
    Frame.Size = UDim2.new(0, 300, 0, 300)
    Frame.Position = UDim2.new(0.5, -150, 0.5, -150)
    Frame.Active = true
    Frame.Visible = true
    UIGradient.Parent = Frame
    UIGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(20, 20, 20)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(50, 50, 50)),
    })

    local TextLabel = Instance.new('TextLabel')

    TextLabel.Parent = Frame
    TextLabel.Size = UDim2.new(0, 300, 0, 30)
    TextLabel.Position = UDim2.new(0, 0, 0, 0)
    TextLabel.BackgroundTransparency = 1
    TextLabel.Text = 'MTX HUB - STAT CHANGER'
    TextLabel.TextColor3 = Color3.new(1, 1, 1)
    TextLabel.TextScaled = true
    TextLabel.TextStrokeTransparency = 0.5
    TextLabel.Font = Enum.Font.SourceSansBold

    local Setting, Setting1, Setting2 = ipairs({
        {
            Name = 'Strength',
            Placeholder = 'Change Strength',
        },
        {
            Name = 'Rebirths',
            Placeholder = 'Change Rebirths',
        },
        {
            Name = 'Kills',
            Placeholder = 'Change Kills',
        },
        {
            Name = 'Brawls',
            Placeholder = 'Change Brawls',
        },
    })

    local function Func(InputValue)
        local InputText = string.upper(InputValue)
        local ConversionTable = {
            T = 1000000000000,
            B = 1000000000,
            M = 1000000,
            K = 1000,
            Qa = 1000000000000000,
        }
        local NumberMatch, SuffixMatch = string.match(InputText, '^(%d+%.?%d*)([TBMKQa]?)$')
        local NumberValue = tonumber(NumberMatch)

        if NumberValue and (SuffixMatch and ConversionTable[SuffixMatch]) then
            return NumberValue * ConversionTable[SuffixMatch]
        else
            return NumberValue
        end
    end
    local function Func1(Parameter2)
        if Parameter2 >= 1000000000000000 then
            return string.format('%.1fQa', Parameter2 / 1000000000000000)
        elseif Parameter2 >= 1000000000000 then
            return string.format('%.1fT', Parameter2 / 1000000000000)
        elseif Parameter2 >= 1000000000 then
            return string.format('%.1fB', Parameter2 / 1000000000)
        elseif Parameter2 >= 1000000 then
            return string.format('%.1fM', Parameter2 / 1000000)
        elseif Parameter2 >= 1000 then
            return string.format('%.1fK', Parameter2 / 1000)
        else
            return tostring(Parameter2)
        end
    end

    local EmptyTable = {}

    while true do
        local UnknownFunc

        Setting2, UnknownFunc = Setting(Setting1, Setting2)

        if Setting2 == nil then
            break
        end

        local TextBox = Instance.new('TextBox')

        TextBox.Parent = Frame
        TextBox.Size = UDim2.new(0, 250, 0, 30)
        TextBox.Position = UDim2.new(0.1, 0, 0.1 + 0.2 * (Setting2 - 1), 0)
        TextBox.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
        TextBox.TextColor3 = Color3.new(1, 1, 1)
        TextBox.TextScaled = true
        TextBox.Font = Enum.Font.SourceSans
        TextBox.Text = ''
        TextBox.PlaceholderText = UnknownFunc.Placeholder
        TextBox.PlaceholderColor3 = Color3.new(0.7, 0.7, 0.7)
        TextBox.ClearTextOnFocus = false

        local UIStroke = Instance.new('UIStroke')

        UIStroke.Parent = TextBox
        UIStroke.Color = Color3.new(1, 1, 1)
        UIStroke.Thickness = 2

        TextBox.FocusLost:Connect(function(Parameter3)
            if Parameter3 then
                local ConvertedText = Func(TextBox.Text)

                if ConvertedText then
                    local Leaderstats = game.Players.LocalPlayer:FindFirstChild('leaderstats')

                    if Leaderstats then
                        local LeaderstatValue = Leaderstats:FindFirstChild(UnknownFunc.Name)

                        if LeaderstatValue and LeaderstatValue:IsA('IntValue') then
                            LeaderstatValue.Value = math.floor(ConvertedText)
                            TextBox.Text = Func1(ConvertedText)
                        end
                    end
                else
                    local Leaderstats1 = game.Players.LocalPlayer:FindFirstChild('leaderstats')

                    if Leaderstats1 then
                        local LeaderstatValue1 = Leaderstats1:FindFirstChild(UnknownFunc.Name)

                        if LeaderstatValue1 and LeaderstatValue1:IsA('IntValue') then
                            TextBox.Text = tostring(LeaderstatValue1.Value)
                        end
                    end
                end
            end
        end)
        table.insert(EmptyTable, TextBox)
    end

    UICorner.CornerRadius = UDim.new(0, 15)
    UICorner.Parent = Frame
    TextButton.Parent = ScreenGui
    TextButton.Size = UDim2.new(0, 100, 0, 50)
    TextButton.Position = UDim2.new(1, -120, 0, 20)
    TextButton.BackgroundColor3 = Color3.new(0, 0, 0)
    TextButton.BackgroundTransparency = 0.5
    TextButton.Text = 'Hide UI'
    TextButton.TextColor3 = Color3.new(1, 1, 1)
    TextButton.TextScaled = true

    local UICorner1 = Instance.new('UICorner')

    UICorner1.CornerRadius = UDim.new(0, 15)
    UICorner1.Parent = TextButton

    TextButton.MouseButton1Click:Connect(function()
        Frame.Visible = not Frame.Visible
        TextButton.Text = Frame.Visible and 'Hide UI' or 'Show UI'
    end)
end)

local MiscTab = UiLibrary:AddTab('Misc')

MiscTab:Show()
MiscTab:AddSwitch('Anti AFK', function(Parameter4)
    while Parameter4 do
        game:GetService('VirtualUser'):CaptureController()
        game:GetService('VirtualUser'):ClickButton2(Vector2.new())
        wait(60)
    end
end):Set(false)
MiscTab:AddSwitch('Night Time', function(Parameter5)
    if Parameter5 then
        game:GetService('Lighting').ClockTime = 0
    else
        game:GetService('Lighting').ClockTime = 14
    end
end):Set(false)
MiscTab:AddSwitch('Anti Lag V2', function(Parameter6)
    if Parameter6 then
        local Descendant, Descendant1, Descendant2 = pairs(game:GetDescendants())

        while true do
            local UnknownValue3

            Descendant2, UnknownValue3 = Descendant(Descendant1, Descendant2)

            if Descendant2 == nil then
                break
            end
            if UnknownValue3:IsA('Part') or (UnknownValue3:IsA('MeshPart') or UnknownValue3:IsA('UnionOperation')) then
                UnknownValue3.Material = Enum.Material.SmoothPlastic
            end
        end
    end
end):Set(false)
MiscTab:AddSwitch('FPS Boost', function(Parameter7)
    if Parameter7 then
        game:GetService('Lighting').GlobalShadows = false
        game:GetService('Lighting').OutdoorAmbient = Color3.new(1, 1, 1)

        local Descendant3, Descendant4, Descendant5 = pairs(game:GetDescendants())

        while true do
            local UnknownValue4

            Descendant5, UnknownValue4 = Descendant3(Descendant4, Descendant5)

            if Descendant5 == nil then
                break
            end
            if UnknownValue4:IsA('BasePart') then
                UnknownValue4.Material = Enum.Material.Plastic
            end
        end
    end
end):Set(false)
MiscTab:AddButton('Infinite Jump', function()
    local LocalPlayer1 = game.Players.LocalPlayer

    LocalPlayer1.Character.Humanoid.JumpPower = 50

    game:GetService('UserInputService').JumpRequest:Connect(function()
        LocalPlayer1.Character:FindFirstChildOfClass('Humanoid'):ChangeState('Jumping')
    end)
end)
MiscTab:AddButton('Reset Character', function()
    local LocalPlayer2 = game.Players.LocalPlayer

    if LocalPlayer2 and LocalPlayer2.Character then
        LocalPlayer2.Character:BreakJoints()
    end
end)
MiscTab:AddButton('Rejoin Server', function()
    game:GetService('TeleportService'):Teleport(game.PlaceId, game.Players.LocalPlayer)
end)
MiscTab:AddButton('Server Hop', function()
    local GameData = game:GetService('HttpService'):JSONDecode(game:HttpGet('https://games.roblox.com/v1/games/' .. game.PlaceId .. '/servers/Public?sortOrder=Asc&limit=100'))
    local GameDataChild, GameDataChild1, GameDataChild2 = pairs(GameData.data)

    while true do
        local UnknownValue5

        GameDataChild2, UnknownValue5 = GameDataChild(GameDataChild1, GameDataChild2)

        if GameDataChild2 == nil then
            break
        end
        if UnknownValue5.playing < UnknownValue5.maxPlayers then
            game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, UnknownValue5.id)

            break
        end
    end
end)

